tool:AnalyticTool a owl:Class ;
    rdfs:label "AnalyticTool" ;
    rdfs:comment "\"An analytic tool is an artifact of hardware and/or software utilized to accomplish a task or purpose of explanation, interpretation or logical reasoning.\"" ;
    rdfs:subClassOf tool:Tool .

tool:BuildFacet a owl:Class ;
    rdfs:label "BuildFacet" ;
    rdfs:comment "\"A build facet is a grouping of characteristics unique to a particular version of a software.\"" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass tool:BuildInformationType ;
            owl:onProperty tool:buildInformation ],
        <https://w3id.org/lmodel/uco-core/Facet> .

tool:ConfiguredTool a owl:Class ;
    rdfs:label "ConfiguredTool" ;
    rdfs:comment "\"A ConfiguredTool is a Tool that is known to be configured to run in a more specified manner than some unconfigured or less-configured Tool.\"" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/lmodel/uco-configuration/Configuration> ;
            owl:onProperty <https://w3id.org/lmodel/uco-configuration/usesConfiguration> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/lmodel/uco-core/UcoObject> ;
            owl:onProperty <https://w3id.org/lmodel/uco-configuration/isConfigurationOf> ],
        tool:Tool .

tool:DefensiveTool a owl:Class ;
    rdfs:label "DefensiveTool" ;
    rdfs:comment "\"A defensive tool is an artifact of hardware and/or software utilized to accomplish a task or purpose of guarding.\"" ;
    rdfs:subClassOf tool:Tool .

tool:MaliciousTool a owl:Class ;
    rdfs:label "MaliciousTool" ;
    rdfs:comment "\"A malicious tool is an artifact of hardware and/or software utilized to accomplish a malevolent task or purpose.\"" ;
    rdfs:subClassOf tool:Tool .

<https://w3id.org/lmodel/uco-configuration/configurationEntry> a owl:ObjectProperty ;
    rdfs:label "configurationEntry" ;
    rdfs:comment "A single configuration setting entry item for a tool or other software." ;
    rdfs:range <https://w3id.org/lmodel/uco-configuration/ConfigurationEntry> ;
    rdfs:seeAlso "https://github.com/ucoProject/UCO/issues/432" .

<https://w3id.org/lmodel/uco-configuration/dependencies> a owl:ObjectProperty ;
    rdfs:label "dependencies" ;
    rdfs:comment "The relevant configuration dependencies for a tool, application, software, or other cyber object." ;
    rdfs:range <https://w3id.org/lmodel/uco-configuration/Dependency> .

<https://w3id.org/lmodel/uco-configuration/dependencyDescription> a owl:DatatypeProperty ;
    rdfs:label "dependencyDescription" ;
    rdfs:comment "A description of a tool or other software dependency." ;
    rdfs:range xsd:string .

<https://w3id.org/lmodel/uco-configuration/dependencyType> a owl:DatatypeProperty ;
    rdfs:label "dependencyType" ;
    rdfs:comment "The type of a tool or other software dependency." ;
    rdfs:range xsd:string ;
    skos:exactMatch <https://w3id.org/sumo/kb/dependencyType> .

<https://w3id.org/lmodel/uco-configuration/isConfigurationOf> a owl:ObjectProperty ;
    rdfs:label "isConfigurationOf" ;
    rdfs:comment "The object which has been configured to run in a more specified manner than another object.  This property is expected to have a more specific range when associated with a class, such as a configured Tool having this property have a range of a Tool." ;
    rdfs:range <https://w3id.org/lmodel/uco-core/UcoObject> ;
    tool:owl "IrreflexiveProperty" .

<https://w3id.org/lmodel/uco-configuration/itemDescription> a owl:DatatypeProperty ;
    rdfs:label "itemDescription" ;
    rdfs:comment "A description of a configuration setting entry item." ;
    rdfs:range xsd:string ;
    skos:broadMatch <http://identifiers.org/sio/000136>,
        <http://identifiers.org/wikidata/Q3024326>,
        ns1:NCIT_C25365,
        <https://w3id.org/lmodel/core/Description> .

<https://w3id.org/lmodel/uco-configuration/itemName> a owl:DatatypeProperty ;
    rdfs:label "itemName" ;
    rdfs:comment "The name of a configuration setting entry item." ;
    rdfs:range xsd:string ;
    skos:broadMatch <http://identifiers.org/sio/000116>,
        <http://identifiers.org/wikidata/Q82799>,
        ns1:NCIT_C42614,
        <https://w3id.org/lmodel/core/Name>,
        <https://w3id.org/sumo/kb/Name> ;
    skos:exactMatch <http://edamontology.org/data_2099> ;
    skos:relatedMatch ns1:NCIT_C95653 .

<https://w3id.org/lmodel/uco-configuration/itemObject> a owl:ObjectProperty ;
    rdfs:label "itemObject" ;
    rdfs:comment "The structured value of a configuration setting entry instance." ;
    rdfs:range <https://w3id.org/lmodel/uco-core/UcoObject> ;
    rdfs:seeAlso "https://github.com/ucoProject/UCO/issues/432>" .

<https://w3id.org/lmodel/uco-configuration/itemType> a owl:DatatypeProperty ;
    rdfs:label "itemType" ;
