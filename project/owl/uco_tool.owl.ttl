        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/latitude> ],
        <https://w3id.org/lmodel/uco-core/Facet> .

<https://w3id.org/lmodel/uco-location/SimpleAddressFacet> a owl:Class ;
