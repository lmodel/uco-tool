<https://w3id.org/lmodel/uco-core/Annotation> a owl:Class ;
    rdfs:label "Annotation" ;
    rdfs:comment "An annotation is an assertion made in relation to one or more objects." ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom <https://w3id.org/lmodel/uco-core/UcoObject> ;
                        owl:onProperty <https://w3id.org/lmodel/uco-core/object> ] [ a owl:Restriction ;
                        owl:onProperty <https://w3id.org/lmodel/uco-core/object> ;
                        owl:someValuesFrom <https://w3id.org/lmodel/uco-core/UcoObject> ] ) ],
        <https://w3id.org/lmodel/uco-core/Assertion> ;
    skos:exactMatch <http://identifiers.org/sio/001166>,
        <http://identifiers.org/wikidata/Q857525>,
        ns1:NCIT_C44272 .

<https://w3id.org/lmodel/uco-core/AttributedName> a owl:Class ;
    rdfs:label "AttributedName" ;
    rdfs:comment "An attributed name is a name of an entity issued by some attributed naming authority." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/namingAuthority> ],
        <https://w3id.org/lmodel/uco-core/UcoObject> ;
    skos:relatedMatch <http://identifiers.org/wikidata/Q32635>,
        <http://identifiers.org/wikidata/Q5508349>,
        <http://identifiers.org/wikidata/Q758251> .

<https://w3id.org/lmodel/uco-core/Bundle> a owl:Class ;
    rdfs:label "Bundle" ;
    rdfs:comment "A bundle is a container for a grouping of UCO content with no presumption of shared context." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-core/EnclosingCompilation> ;
    skos:closeMatch <http://identifiers.org/wikidata/Q1643805>,
        <http://identifiers.org/wikidata/Q17008256> ;
    skos:narrowMatch <http://identifiers.org/wikidata/Q1995545>,
        ns1:NCIT_C54192 .

<https://w3id.org/lmodel/uco-core/ConfidenceFacet> a owl:Class ;
    rdfs:label "ConfidenceFacet" ;
    rdfs:comment "A confidence is a grouping of characteristics unique to an asserted level of certainty in the accuracy of some information." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:nonNegativeInteger ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/confidence> ;
            owl:qualifiedCardinality 1 ],
        <https://w3id.org/lmodel/uco-core/Facet> ;
    skos:relatedMatch <https://w3id.org/lmodel/core/ConfidenceLevel> .

<https://w3id.org/lmodel/uco-core/ControlledVocabulary> a owl:Class ;
    rdfs:label "ControlledVocabulary" ;
    rdfs:comment "A controlled vocabulary is an explicitly constrained set of string values." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/constrainingVocabularyReference> ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/value> ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/constrainingVocabularyName> ],
        <https://w3id.org/lmodel/uco-core/UcoObject> ;
    skos:exactMatch <http://identifiers.org/wikidata/Q1469824>,
        ns1:NCIT_C48697 .

<https://w3id.org/lmodel/uco-core/Grouping> a owl:Class ;
    rdfs:label "Grouping" ;
    rdfs:comment "A grouping is a compilation of referenced UCO content with a shared context." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/context> ],
        <https://w3id.org/lmodel/uco-core/ContextualCompilation> ;
    skos:exactMatch <http://identifiers.org/wikidata/Q71468208>,
        ns1:NCIT_C25523 .

<https://w3id.org/lmodel/uco-core/Item> a owl:Class ;
    rdfs:label "Item" ;
    rdfs:comment "An item is a distinct article or unit." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-core/UcoObject> ;
    skos:exactMatch <http://identifiers.org/wikidata/Q11723795> ;
    skos:narrowMatch <http://identifiers.org/wikidata/Q3409269>,
        ns1:NCIT_C142592 .

<https://w3id.org/lmodel/uco-core/ModusOperandi> a owl:Class ;
    rdfs:label "ModusOperandi" ;
    rdfs:comment "A modus operandi is a particular method of operation (how a particular entity behaves or the resources they use)." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-core/UcoObject> ;
    skos:exactMatch <http://identifiers.org/wikidata/Q301011> .

<https://w3id.org/lmodel/uco-core/Relationship> a owl:Class ;
    rdfs:label "Relationship" ;
    rdfs:comment "A relationship is a grouping of characteristics unique to an assertion that one or more objects are related to another object in some way." ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom <https://w3id.org/lmodel/uco-core/UcoObject> ;
                        owl:onProperty <https://w3id.org/lmodel/uco-core/source> ] [ a owl:Restriction ;
                        owl:onProperty <https://w3id.org/lmodel/uco-core/source> ;
                        owl:someValuesFrom <https://w3id.org/lmodel/uco-core/UcoObject> ] ) ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/endTime> ],
        [ a owl:Restriction ;
            owl:onClass <https://w3id.org/lmodel/uco-core/UcoObject> ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/target> ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/kindOfRelationship> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/startTime> ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty <https://w3id.org/lmodel/uco-core/isDirectional> ;
            owl:qualifiedCardinality 1 ],
        <https://w3id.org/lmodel/uco-core/UcoObject> ;
    skos:exactMatch <http://identifiers.org/wikidata/Q930933>,
        ns1:NCIT_C25648 .

<https://w3id.org/lmodel/uco-identity/AddressFacet> a owl:Class ;
    rdfs:label "AddressFacet" ;
    rdfs:comment "An Address Facet is a grouping of characteristics unique to an administrative identifier for a geolocation associated with a specific identity." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/lmodel/uco-location/Location> ;
            owl:onProperty <https://w3id.org/lmodel/uco-identity/address> ],
        <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/AffiliationFacet> a owl:Class ;
    rdfs:label "AffiliationFacet" ;
    rdfs:comment "An affiliation is a grouping of characteristics unique to the established affiliations of an entity." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/BirthInformationFacet> a owl:Class ;
    rdfs:label "BirthInformationFacet" ;
    rdfs:comment "Birth information is a grouping of characteristics unique to information pertaining to the birth of an entity." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:dateTime ;
            owl:onProperty <https://w3id.org/lmodel/uco-identity/birthdate> ],
        <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/CountryOfResidenceFacet> a owl:Class ;
    rdfs:label "CountryOfResidenceFacet" ;
    rdfs:comment "Country of residence is a grouping of characteristics unique to information related to the country, or countries, where an entity resides." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/EventsFacet> a owl:Class ;
    rdfs:label "EventsFacet" ;
    rdfs:comment "Events is a grouping of characteristics unique to information related to specific relevant things that happen in the lifetime of an entity." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/IdentifierFacet> a owl:Class ;
    rdfs:label "IdentifierFacet" ;
    rdfs:comment "Identifier is a grouping of characteristics unique to information that uniquely and specifically identities an entity." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/LanguagesFacet> a owl:Class ;
    rdfs:label "LanguagesFacet" ;
    rdfs:comment "Languages is a grouping of characteristics unique to specific syntactically and grammatically standardized forms of communication (human or computer) in which an entity has proficiency (comprehends, speaks, reads, or writes)." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/NationalityFacet> a owl:Class ;
    rdfs:label "NationalityFacet" ;
    rdfs:comment "Nationality is a grouping of characteristics unique to the condition of an entity belonging to a particular nation." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/OccupationFacet> a owl:Class ;
    rdfs:label "OccupationFacet" ;
    rdfs:comment "Occupation is a grouping of characteristics unique to the job or profession of an entity." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/Organization> a owl:Class ;
    rdfs:label "Organization" ;
    rdfs:comment "An organization is a grouping of identifying characteristics unique to a group of people who work together in an organized way for a shared purpose. [based on https://dictionary.cambridge.org/us/dictionary/english/organization]" ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/Identity> ;
    skos:exactMatch <http://identifiers.org/sio/000012>,
        <http://identifiers.org/wikidata/Q15911314>,
        <http://identifiers.org/wikidata/Q43229>,
        ns1:GSSO_000232,
        <https://w3id.org/sumo/kb/Organization> .

<https://w3id.org/lmodel/uco-identity/OrganizationDetailsFacet> a owl:Class ;
    rdfs:label "OrganizationDetailsFacet" ;
    rdfs:comment "Organization details is a grouping of characteristics unique to an identity representing an administrative and functional structure." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/Person> a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A person is a grouping of identifying characteristics unique to a human being regarded as an individual. [based on https://www.lexico.com/en/definition/person]" ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/Identity> ;
    skos:exactMatch <http://identifiers.org/sio/000498>,
        <http://identifiers.org/wikidata/Q215627>,
        ns1:NCIT_C25190 ;
    skos:narrowMatch <https://w3id.org/sumo/kb/PersonID> .

<https://w3id.org/lmodel/uco-identity/PersonalDetailsFacet> a owl:Class ;
    rdfs:label "PersonalDetailsFacet" ;
    rdfs:comment "Personal details is a grouping of characteristics unique to an identity representing an individual person." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/PhysicalInfoFacet> a owl:Class ;
    rdfs:label "PhysicalInfoFacet" ;
    rdfs:comment "Physical info is a grouping of characteristics unique to the outwardly observable nature of an individual person." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/QualificationFacet> a owl:Class ;
    rdfs:label "QualificationFacet" ;
    rdfs:comment "Qualification is a grouping of characteristics unique to particular skills, capabilities or their related achievements (educational, professional, etc.) of an entity." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/RelatedIdentityFacet> a owl:Class ;
    rdfs:label "RelatedIdentityFacet" ;
    rdfs:comment "<Needs fleshed out from CIQ>" ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/SimpleNameFacet> a owl:Class ;
    rdfs:label "SimpleNameFacet" ;
    rdfs:comment "A simple name facet is a grouping of characteristics unique to the personal name (e.g., Dr. John Smith Jr.) held by an identity." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-identity/honorificPrefix> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-identity/familyName> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-identity/honorificSuffix> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <https://w3id.org/lmodel/uco-identity/givenName> ],
        <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-identity/VisaFacet> a owl:Class ;
    rdfs:label "VisaFacet" ;
    rdfs:comment "Visa is a grouping of characteristics unique to information related to a person's ability to enter, leave, or stay for a specified period of time in a country." ;
    rdfs:subClassOf <https://w3id.org/lmodel/uco-identity/IdentityFacet> .

<https://w3id.org/lmodel/uco-location/GPSCoordinatesFacet> a owl:Class ;
    rdfs:label "GPSCoordinatesFacet" ;
    rdfs:comment "A GPS coordinates facet is a grouping of characteristics unique to the expression of quantified dilution of precision (DOP) for an asserted set of geolocation coordinates typically associated with satellite navigation such as the Global Positioning System (GPS)." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/pdop> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/tdop> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/vdop> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:decimal ;
            owl:onProperty <https://w3id.org/lmodel/uco-location/hdop> ],
        <https://w3id.org/lmodel/uco-core/Facet> .

<https://w3id.org/lmodel/uco-location/LatLongCoordinatesFacet> a owl:Class ;
    rdfs:label "LatLongCoordinatesFacet" ;
    rdfs:comment "A lat long coordinates facet is a grouping of characteristics unique to the expression of a geolocation as the intersection of specific latitude, longitude, and altitude values." ;
    rdfs:subClassOf [ a owl:Restriction ;
